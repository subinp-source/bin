        <typegroup name="Apparel">
            <itemtype code="ApparelProduct" extends="Product"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ApparelProduct">
                <description>Base apparel product extension that contains additional attributes.</description>
                <attributes>

                    <attribute qualifier="genders" type="GenderList">
                        <description>List of genders that the ApparelProduct is designed for</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="testAttribute" type="localized:java.lang.String">
                        <description>labelTestAttribute</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>





                </attributes>
            </itemtype>

            <itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ApparelStyleVariantProduct">
                <description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
                <attributes>
                    <attribute qualifier="style" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color/Pattern of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="swatchColors" type="SwatchColorSet">
                        <description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>







                </attributes>

            </itemtype>

            <itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ApparelSizeVariantProduct">
                <description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
                <attributes>
                    <attribute qualifier="size" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Size of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ElectronicsColorVariantProduct">
                <description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>





        <typegroup name="Vehicle">
            <itemtype code="VehicleBase"  extends="Product" autocreate="true" generate="true" jaloclass="de.hybris.training.core.jalo.VehicleBase">
                <attributes>
                 <attribute qualifier="vehicleRegistrationNumber" type="java.lang.String">

                     <defaultvalue/>
                     <description>vehicle number</description>
                        <modifiers read="true" write="true"  unique="true" optional="false" />
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="vehicleId" type="java.lang.String">

                        <defaultvalue/>
                        <description>vehicle Id</description>
                        <modifiers read="true" write="true"  unique="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>





                    <!--<attribute qualifier="registrationNumber" type="java.lang.String">

                        <description>vehicle number</description>
                        <modifiers read="true" write="true"  unique="true" />
                        <persistence type="property"/>
                    </attribute>-->
                    <attribute qualifier="vehicleDescription" type="localized:java.lang.String">
                        <description>vehicle description</description>
                        <modifiers read="true" write="true" optional="true" unique="true"/>
                        <persistence type="property"/>
                    </attribute>



                    <attribute qualifier="vehicleReview" type="localized:java.lang.String">
                        <description>vehicle review</description>
                        <modifiers read="true" write="true" optional="true" unique="true"/>
                        <persistence type="property"/>
                    </attribute>

                </attributes>
            </itemtype>


            <itemtype code="DealerBase" autocreate="true" generate="true" jaloclass="de.hybris.training.core.jalo.DealerBase">


                <deployment table="DealerBase" typecode="12346"/>
                <attributes>
                    <attribute qualifier="address" type="localized:java.lang.String">
                        <description>dealer address</description>
                        <modifiers read="true" write="true" optional="true" />
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="localizeName" type="localized:java.lang.String">
                        <description>name of the dealer</description>
                        <modifiers read="true" write="true" optional="true" />
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="uniqueId" type="java.lang.String">
                        <defaultvalue>0001</defaultvalue>
                        <description>dealer of unique id</description>
                        <modifiers read="true" write="true" optional="false" unique="true"/>
                        <persistence type="property"/>
                    </attribute>



                </attributes>
            </itemtype>



            <itemtype code="DriverBase" autocreate="true" generate="true" jaloclass="de.hybris.training.core.jalo.DriverBase">


                <deployment table="DriverBase" typecode="12347"/>
                <attributes>
                    <attribute qualifier="nameOfDriver" type="localized:java.lang.String">
                        <description>driver name</description>
                        <modifiers read="true" write="true" optional="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="driverId" type="java.lang.String">
                        <defaultvalue/>
                        <description>id of driver</description>
                        <modifiers read="true" write="true" optional="false" unique="true"/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="experience" type="java.lang.String">
                        <description>experience in years</description>
                        <modifiers read="true" write="true" optional="true"/>
                        <persistence type="property"/>
                    </attribute>



                </attributes>
            </itemtype>



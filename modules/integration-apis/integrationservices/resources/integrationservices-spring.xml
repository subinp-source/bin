        <property name="attributeValueSetterFactory" ref="integrationServicesAttributeValueSetterFactory"/>
        <property name="attributeSettableCheckerFactory" ref="integrationServicesAttributeSettableCheckerFactory"/>
    </bean>

    <alias name="defaultAttributeValueAccessorFactory" alias="integrationServicesAttributeValueAccessorFactory"/>

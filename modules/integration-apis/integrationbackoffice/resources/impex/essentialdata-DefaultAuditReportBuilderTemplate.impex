	<script>
		//<![CDATA[
		ADDED = 'addition';
		DELETED = 'deletion';
		UPDATED = 'update';
		ITEM_TYPE_CONFIG_KEY = 'ComposedTypeOfItem';
        COMPOSED_TYPE_CONFIG_KEY = 'ComposedType_';
		function convertArrayToObject(array) {
			var obj = {};
			if (!isArray(array)) {
				return array;
			}
			for (var i = 0; i < array.length; i++) {
				if (typeof array[i] === 'object' && 'code' in array[i] && 'IntegrationObjectItemClassification' in array[i]) { // ensure array[i] is IOItem
					obj['itemName : ' + array[i]['code']] = array[i];
					delete array[i]['code'];
				}
				else if (typeof array[i] === 'object' && 'attributeName' in array[i]) {
				    if('attributeName : ' + array[i]['attributeName'] in obj){
						alert(array[i]['attributeName'] + 'is duplicate as attributeName in ' + obj.toString())
					}
					obj['attributeName : ' + array[i]['attributeName']] = array[i];
					delete array[i]['attributeName'];
				}
				else {
					obj[i + 1] = array[i];
				}
			}
			return obj;
		}
		function adjustOjbectStruture(obj) {
			if(!isObject(obj)){
				return
			}
			if(ITEM_TYPE_CONFIG_KEY in obj && isObject(obj[ITEM_TYPE_CONFIG_KEY])
				&& COMPOSED_TYPE_CONFIG_KEY in obj[ITEM_TYPE_CONFIG_KEY]){
					obj[COMPOSED_TYPE_CONFIG_KEY] = obj[ITEM_TYPE_CONFIG_KEY][COMPOSED_TYPE_CONFIG_KEY].toString()
					delete obj[ITEM_TYPE_CONFIG_KEY]
			}
		}
		function Value(value, oldValue, type) {
			this.__newValue = value;
			this.__oldValue = oldValue
			this.__type = type
		}
		Value.prototype.toString = function () {
			if (this.__type === DELETED && this.__oldValue !== undefined) {
				return this.__oldValue
			}
			return this.__newValue
		}
		function traverse(a_payload) {
			var stack = [];
			stack.push(a_payload);
			while (stack.length !== 0) {
				var current = stack.shift();
				for (var key in current) {
					if (isObject(current[key]) || isArray(current[key])) {
						if (isArray(current[key])) {
							current[key] = convertArrayToObject(current[key])
						}
						else if(isObject(current[key])){
							adjustOjbectStruture(current[key])
						}
						stack.push(current[key])
					}
					else if (isPrimitive(current[key]) && current[key] !== undefined) {
						current[key] = new Value(current[key])
					}
				}
			}
		}
		function merge(oldObj, newObj) {
			if (oldObj === undefined && newObj === undefined) {
				throw 'Something wrong.'
			}
			// if oldObj is undefined, that means the key for oldObj not exists. So the newObj is a new value with a new key.
			// if oldObj is null, that means the key for oldObj exists but no value. It is likely a optional property. Happens when crawling some objects.
			// null is a value.
			if (oldObj === undefined) {
			    if(isValue(newObj)){
					newObj['__type'] = ADDED
					return newObj
				}
				return {
					'__newValue': newObj,
					'__type': ADDED
				}
			}
			// Same as oldObj === undefined logic.
			if (newObj === undefined) {

			    if(isValue(oldObj)){
					oldObj['__type'] = DELETED
					return oldObj
				}
				return {
					'__oldValue': oldObj,
					'__type': DELETED
				}
			}
			// all primitives are converted to Value. Just in case.
			if(isPrimitive(newObj)){
				return oldObj === newObj? new Value(newObj, undefined, undefined) : new Value(newObj, undefined, UPDATED)
			}
			if (isValue(oldObj) && isValue(newObj)) {
				if (oldObj.__newValue !== newObj.__newValue) {
					return new Value(newObj.__newValue, oldObj.__newValue, UPDATED)
				}
				return newObj
			}
			if (isValue(oldObj) ^ isValue(newObj)) {
				if(isPrimitive(newObj)){
					return new Value(newObj, undefined, UPDATED)
				}
				else if(isValue(newObj) || isObject(newObj)){
					newObj['__type'] = UPDATED
					return newObj
				}
			}
			if(isPrimitive(oldObj)){  // newObj must be an object
				newObj['__type'] = UPDATED
				return newObj
			}
			var mergedObj = {}
			for (var key1 in oldObj) {
				mergedObj[key1] = merge(oldObj[key1], newObj[key1])
			}
			for (var key2 in newObj) {
				if (!(key2 in mergedObj)) {
					mergedObj[key2] = merge(oldObj[key2], newObj[key2])
				}
			}
			return mergedObj
		}
		function isArray(x) {
			return Object.prototype.toString.call(x) === '[object Array]';
		}
		function isObject(x) {
			return Object.prototype.toString.call(x) === '[object Object]';
		}
		function isPrimitive(x) {
			return !isObject(x);
		}
		function isValue(x) {
			return x instanceof Value;
		}
		function toggle(source) {
			if (source) {
				var tabs = document.querySelectorAll('[aria-selected]')
				for (i = 0; i < tabs.length; i++) {
					tabs[i].setAttribute('aria-selected', 'false')
				}
				source.setAttribute('aria-selected', 'true')
				if (source.classList.contains('collapsed')) {
					source.classList.remove('collapsed')
					source.classList.add('expanded')
					source.setAttribute('aria-expanded', 'true')
				} else {
					source.classList.add('collapsed')
					source.classList.remove('expanded')
					source.setAttribute('aria-expanded', 'false')
				}
			}
		}
		function convertToHtml(elem, included, inherit) {
			if (elem == null) {
				return convertNullToHtml()
			} else if (elem.constructor === Array) {
				return convertArrayToHtml(elem)
			} else if (isValue(elem)) {
				// return inherit === '' ? convertValueToHtml(elem.__newValue, inherit) :
				// 	(inherit === DELETED) ? convertValueToHtml(elem.__oldValue, inherit) : convertValueToHtml(elem.__newValue, inherit)
				return convertValueToHtml(elem.toString(), inherit || elem.__type || '')
			} else if (isObject(elem)) {
				return convertObjectToHtml(elem, inherit)
			} else {
				return convertPrimitiveToHtml(elem, included, inherit)
			}
		}
		function convertNullToHtml() {
			return '<div class=\""null-element""> null</div>'
		}
		function convertValueToHtml(value, inherit) {
			var html = `<div class=""element-value ${inherit} `
            html += typeof value === 'boolean' ? ' boolean-element' : ''
            html += '""'
			html += ' role=""tabpanel""'
			html += '> ' + value + '</div>'
			return html
		}
		function convertArrayToHtml(array) {
			if (array.length > 0) {
				var html = ''
				for (var i = 0; i < array.length; i++) {
					var obj = {}
					if ('code' in array[i]) {
						obj['itemName : ' + array[i]['code']] = array[i]
						delete array[i]['code']
					}
					else if ('attributeName' in array[i]) {
						obj['attributeName : ' + array[i]['attributeName']] = array[i]
						delete array[i]['attributeName']
					}
					else {
						obj[i + 1] = array[i]
					}
					html += convertToHtml(obj)
				}
				return html
			}
			return '<div class=""empty-table""> [ ]</div>'
		}
		function convertObjectToHtml(object, inherit) {
			if (Object.keys(object).length === 0) {
				return '<div class=""empty-object""> </div>'
			}
			var realObj = object
			var colorTag = ''
			if (inherit !== undefined && inherit !== '') {
				colorTag = inherit
			}
			else if (object.__type !== undefined && object.__type !== '') {
				colorTag = object.__type
			}
			// this is unpack for object {__newValue: ..., __oldValue: ...}. Avoid key = '__newValue' or '__oldValue'
			if (colorTag === ADDED) {
				realObj = object.__newValue !== undefined ? object.__newValue : object
			}
			else if (colorTag === DELETED) {
				realObj = object.__oldValue !== undefined ? object.__oldValue : object
			}
			else if (colorTag === UPDATED) {
				realObj = object.__newValue !== undefined ? object.__newValue : object
			}
			var html = '<div class=""element"" role=""tablist"">'
			for (var key of Object.keys(realObj).sort()){
			    if(key === '__type' || key === '__newValue' || key === '__oldValue'){
					continue  // shouldn't be here
				}
			    if(key === 'IntegrationObjectItemClassification' && isObject(realObj['IntegrationObjectItemClassification'])
			    && Object.keys(realObj['IntegrationObjectItemClassification']).length === 0){
					continue
				}
				if(key === 'VirtualAttribute' && isObject(realObj['VirtualAttribute'])
			    && Object.keys(realObj['VirtualAttribute']).length === 0){
					continue
				}
				var subcolorTag = colorTag || ''
				if(isObject(realObj[key]) && '__type' in realObj[key]){
					subcolorTag = subcolorTag || realObj[key]['__type']
				}
				if (isObject(realObj[key]) && !isValue(realObj[key]) && realObj[key] !== null) {
					html += `<div class=""element-node element-name expanded ${subcolorTag}"" onClick=""toggle(this);"" role=""tab""><a>` + key + '</a></div>';
					html += `<div class=""element-content ${subcolorTag}"" role=""tabpanel"">` + convertToHtml(realObj[key], false, subcolorTag) + '</div>';
				} else if (isValue(realObj[key])) {
					html += `<div class=""element-name ${subcolorTag}""><a role=""tabpanel"">` + key + '</a>' + convertToHtml(realObj[key], true, subcolorTag) + '</div>';
				} else {
					html += `<div class=""element-name ${subcolorTag}""><a role=""tabpanel"">` + key + '</a>' + convertToHtml(realObj[key], true, subcolorTag) + '</div>';
				}
			}

			html += '</div>'
			return html
		}
		function convertPrimitiveToHtml(value, included, inherit) {
			var html = `<div class=""element-value ${inherit}`
            html += typeof value === 'boolean' ? ' boolean-element' : ''
            html += '""'
			html += included === true ? ' role=""tabpanel""' : ''
			html += '> ' + value + '</div>'
			return html
		}
		function timestampToDate(obj) {
			if ('timestamp' in obj) {
				// obj['timestamp'] = new Date(obj['timestamp']).toLocaleString()
				obj['timestamp'] = new Date(obj['timestamp']).toLocaleString() + '  ' + obj['timestamp']   // for debug
			}
		}
		function init() {
			var html = ''
			var lastPayload = {}
			// One operation on IO will be decomposed to multiple related payloads while user only want see one payload diff for one change.
			// So for a series of payloads we only show the last one which is the final result.
			// The related payloads usually generated closely in 500ms but there are exceptions. Here threshold is set larger to make sure won't miss anything and can be adjusted later.
			var intervalThreshold = 3000
			var currentPayload = {}
			if (reports == null || !reports instanceof Array) {
				html += convertNullToHtml()
			} else {
				for (var i = 0; i < reports.length; ++i) {
					if (reports[i]['payload'] === undefined) {
						reports[i]['payload'] = {}
					}
					if ('context' in reports[i]) {
						delete reports[i]['context']
					}
					traverse(reports[i]['payload'])
					currentPayload = reports[i]['payload']
					reports[i][""payload""] = merge(lastPayload, reports[i]['payload'])
					timestampToDate(reports[i])
					html = convertToHtml(reports[i]) + html
					lastPayload = currentPayload
				}
			}
			document.getElementById('report').innerHTML = html
			var cells = document.querySelectorAll('#report > .element > .element-node.element-name.expanded')
			for (var index = 0; index < cells.length; index++) {
				cells[index].classList.add('collapsed')
				cells[index].classList.remove('expanded')
			}
		}
		//]]>
	</script>
</head>

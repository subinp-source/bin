<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="de.hybris.platform.integrationbackoffice.widgets.IntegrationObjectButtonPanel">
    <name>Integration Object Button Panel</name>
    <description>The buttons available in the IntegrationObject Editor</description>
    <defaultTitle>Integration Object Button Panel</defaultTitle>
    <author>Hybris</author>
    <version>1.0</version>
    <view src="IntegrationObjectButtonPanel.zul"/>
    <keywords>
        <keyword>IntegrationObjectButtonPanel</keyword>
    </keywords>
    <controller
            class="de.hybris.platform.integrationbackoffice.widgets.editor.controllers.IntegrationObjectButtonPanelController"/>

    <sockets>
        <input type="de.hybris.platform.integrationservices.model.IntegrationObjectModel"
               id="receiveIntegrationObjectComboBox"/>
        <input type="de.hybris.platform.integrationbackoffice.widgets.editor.data.IntegrationFilterState"
               id="filterStateInput"/>
        <input type="java.lang.Boolean" id="enableSaveButtonEvent"/>
        <output type="java.lang.String" id="refreshButtonClick"/>
        <output type="java.lang.String" id="saveButtonClick"/>
        <output type="de.hybris.platform.integrationbackoffice.widgets.editor.data.IntegrationFilterState"
                id="filterStateOutput"/>
    </sockets>
</widget-definition>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="merchandisingservicesbackofficeLabelLocator" class="com.hybris.cockpitng.util.labels.ResourcesLabelLocator" scope="singleton" init-method="init" lazy-init="false">
  		<property name="location" value="/merchandisingservicesbackoffice-backoffice-labels/"/>
  		<property name="name" value="labels"/>
  	</bean>
  	
  	<bean id="merchandisingRollupStrategySearchFacade" class="de.hybris.platform.merchandisingservicesbackoffice.editor.facade.MerchandisingRollupStrategySearchFacade">
		<property name="rollupStrategies" ref="merchRollupStrategiesList"/>
	</bean>

</beans>

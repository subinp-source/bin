	<context component="pcmbackoffice-catalogFilterTree">
		<et:explorer-tree>
			<et:dynamic-node id="Catalogs" indexing-depth="15" populator-bean-id="catalogTreeModelPopulator"
							 expanded-by-default="true" hide-root-node="true">
				<et:dynamic-node-parameter>
					<et:name>simpleLabels</et:name>
					<et:value>true</et:value>
				</et:dynamic-node-parameter>
			</et:dynamic-node>
		</et:explorer-tree>
	</context>

	<context component="pcmbackoffice-catalogFilterTree-searchandbrowse" parent="pcmbackoffice-catalogFilterTree" merge-by="component">
		<et:explorer-tree>
			<et:dynamic-node id="Catalogs" indexing-depth="15" populator-bean-id="catalogTreeModelPopulator"
							 expanded-by-default="true" hide-root-node="true">
				<et:dynamic-node-parameter>
					<et:name>simpleLabels</et:name>
					<et:value>true</et:value>
				</et:dynamic-node-parameter>
				<et:dynamic-node-parameter>
					<et:name>treeLookupAllowForFlatList</et:name>
					<et:value>true</et:value>
				</et:dynamic-node-parameter>
			</et:dynamic-node>
		</et:explorer-tree>
	</context>


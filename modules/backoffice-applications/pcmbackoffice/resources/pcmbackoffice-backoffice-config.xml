	<context type="CatalogVersion" component="create-wizard">
		<wz:flow id="CatalogVersionWizard" title="create.title(ctx.TYPE_CODE)"
		>
			<wz:prepare id="catalogVersionPrepare">
				<wz:initialize property="newCatalogVersion" type="ctx.TYPE_CODE"/>
				<wz:assign property="newCatalogVersion.catalog" value="ctx?.actionContext?.catalog"/>
			</wz:prepare>
			<wz:step id="step1" label="name.your(ctx.TYPE_CODE)" position="10">
				<wz:content id="step1.content">
					<wz:property id="version" qualifier="newCatalogVersion.version" readonly="false" position="10"/>
					<wz:property id="catalog" qualifier="newCatalogVersion.catalog" readonly="false" position="20"/>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:done>
						<wz:save property="newCatalogVersion"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>

	<context type="Category" component="create-wizard">
		<wz:flow id="CategoryWizard" title="create.title(ctx.TYPE_CODE)"
		>
			<wz:prepare id="categoryPrepare">
				<wz:initialize property="newCategory" type="ctx.TYPE_CODE"/>
				<wz:assign property="newCategory.catalogVersion" value="ctx?.actionContext?.catalogVersion"/>
				<wz:assign property="newCategory.supercategories" value="ctx?.actionContext?.supercategories"/>
			</wz:prepare>
			<wz:step id="step1" label="name.your(ctx.TYPE_CODE)">
				<wz:content id="step1.content">
					<wz:property id="code" qualifier="newCategory.code" readonly="false"/>
					<wz:property id="name" qualifier="newCategory.name" readonly="false"/>
					<wz:property id="catalogVersion" qualifier="newCategory.catalogVersion" readonly="false"/>
					<wz:property id="supercategories" qualifier="newCategory.supercategories" readonly="false"/>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:done>
						<wz:save property="newCategory"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>

	<context type="Catalog" component="create-wizard">

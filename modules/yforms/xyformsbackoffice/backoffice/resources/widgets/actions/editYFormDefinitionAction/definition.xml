<action-definition id="de.hybris.platform.xyformsbackoffice.actions.edit"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">
 
   <name>Edit yForm Definition</name>
   <description>Edits a yForm Definition by calling Form Builder</description>
	<author>hybris</author>
	<version>1.0</version>
	<actionClassName>de.hybris.platform.xyformsbackoffice.actions.edit.EditYFormDefinitionAction</actionClassName>

   <sockets>

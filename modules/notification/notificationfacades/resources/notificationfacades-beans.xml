<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	
	<bean class="de.hybris.platform.notificationfacades.data.NotificationPreferenceData">
		<property name="emailEnabled" type="boolean" deprecated="true" deprecatedSince="1905"/>
		<property name="smsEnabled" type="boolean" deprecated="true" deprecatedSince="1905"/>
		<property name="emailAddress" type="java.lang.String" deprecated="true" deprecatedSince="1905"/>
		<property name="mobileNumber" type="java.lang.String" deprecated="true" deprecatedSince="1905"/>
		<property name="channel" type="de.hybris.platform.notificationservices.enums.NotificationChannel"/>
		<property name="value" type="java.lang.String"/>
		<property name="enabled" type="boolean"/>
		<property name="visible" type="boolean"/>
	</bean>
	
	<bean class="de.hybris.platform.notificationfacades.data.NotificationPreferenceDataList">
		<property name="preferences" type="java.util.List&lt;de.hybris.platform.notificationfacades.data.NotificationPreferenceData>"/>
	</bean>
	
	<bean class="de.hybris.platform.notificationfacades.data.SiteMessageData">
		<property name="uid" type="java.lang.String"/>
		<property name="title" type="java.lang.String" deprecated="true" deprecatedSince="1905"/>
		<property name="content" type="java.lang.String" deprecated="true" deprecatedSince="1905"/>
		<property name="subject" type="java.lang.String"/>
		<property name="body" type="java.lang.String"/>
		<property name="link" type="java.lang.String"/>
		<property name="notificationType" type="de.hybris.platform.notificationservices.enums.NotificationType"/>
		<property name="sentDate" type="java.util.Date"/>
	</bean>

</beans>

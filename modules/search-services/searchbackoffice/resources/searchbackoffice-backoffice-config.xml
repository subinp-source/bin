	<!-- Explorer Tree configuration -->
	<context merge-by="module" parent="auto" component="explorer-tree">
		<explorer-tree:explorer-tree>
			<explorer-tree:navigation-node id="hmc_treenode_system" merge-mode="append">
				<explorer-tree:navigation-node id="hmc.solrfacetsearch">
					<explorer-tree:type-node code="AbstractSnSearchProviderConfiguration"
						id="hmc_type_tree_snsearchprovider" />
					<explorer-tree:type-node code="SnIndexConfiguration"
						id="hmc_type_tree_snindexconfiguration" />
					<explorer-tree:type-node code="SnIndexType" id="hmc_type_tree_snindextype" />
					<explorer-tree:type-node code="SnSynonymDictionary" id="hmc_type_tree_snsynonymdictionary" />
				</explorer-tree:navigation-node>
			</explorer-tree:navigation-node>
		</explorer-tree:explorer-tree>
	</context>

	<context type="AbstractSnSearchProviderConfiguration">
		<context merge-by="type" parent="GenericItem" component="base">
			<y:base>
				<y:labels>
					<y:label>(name ?: '') + ' [' + (id ?: '') + ']'</y:label>
				</y:labels>
			</y:base>
		</context>

		<context merge-by="type" parent="GenericItem" component="simple-search">
			<simple-search:simple-search>
				<simple-search:field name="id" />
				<simple-search:field name="name" />
			</simple-search:simple-search>
		</context>

		<context merge-by="type" parent="GenericItem" component="listview">
			<list-view:list-view refresh-after-object-creation="true">
				<list-view:column qualifier="id" />
				<list-view:column qualifier="name" />
			</list-view:list-view>
		</context>

		<context merge-by="type" parent="GenericItem" component="editor-area">
			<editorArea:editorArea>
				<editorArea:essentials>
					<editorArea:essentialSection name="hmc.essential">
						<editorArea:attribute qualifier="id" />
						<editorArea:attribute qualifier="name" />
					</editorArea:essentialSection>
				</editorArea:essentials>
				<editorArea:tab name="tab.configuration.snsearchproviderconfiguration.general" />
			</editorArea:editorArea>
		</context>

		<context component="create-wizard">
			<wz:flow id="snsearchproviderconfiguration-create-wizard" title="create.title(ctx.TYPE_CODE)">
				<wz:prepare id="prepare">
					<wz:initialize property="newItem" type="ctx.TYPE_CODE" />
				</wz:prepare>
				<wz:step id="step1" label="create.searchproviderconfiguration.essential.label"
					sublabel="create.searchproviderconfiguration.essential.sublabel">
					<wz:content id="step1.content">
						<wz:property-list root="newItem">
							<wz:property qualifier="id" />
							<wz:property qualifier="name" />
						</wz:property-list>
					</wz:content>
					<wz:navigation id="step1.navigation">
						<wz:cancel />
						<wz:done visible="newItem.id != null">
							<wz:save property="newItem" />
						</wz:done>
					</wz:navigation>
				</wz:step>
			</wz:flow>
		</context>
	</context>

	<context type="SnIndexConfiguration">
		<context merge-by="type" parent="GenericItem" component="base">
			<y:base>
				<y:labels>
					<y:label>(name ?: '') + ' [' + (id ?: '') + ']'</y:label>
				</y:labels>
			</y:base>
		</context>

		<context merge-by="type" parent="GenericItem" component="simple-search">
			<simple-search:simple-search>
				<simple-search:field name="id" />
				<simple-search:field name="name" />
			</simple-search:simple-search>
		</context>

		<context merge-by="type" parent="GenericItem" component="listview">
			<list-view:list-view refresh-after-object-creation="true">
				<list-view:column qualifier="id" />
				<list-view:column qualifier="name" />
				<list-view:column qualifier="searchProviderConfiguration" />
			</list-view:list-view>
		</context>

		<context merge-by="type" parent="GenericItem" component="editor-area">
			<editorArea:editorArea>
				<editorArea:essentials>
					<editorArea:essentialSection name="hmc.essential">
						<editorArea:attribute qualifier="id" />
						<editorArea:attribute qualifier="name" />
					</editorArea:essentialSection>
				</editorArea:essentials>
				<editorArea:tab name="tab.configuration.snindexconfiguration.general">
					<editorArea:section name="sec.configuration.snindexconfiguration.general.searchandindexer">
						<editorArea:attribute qualifier="searchProviderConfiguration" />
					</editorArea:section>
					<editorArea:section name="sec.configuration.snindexconfiguration.general.synonymdictionaries">
						<editorArea:attribute qualifier="synonymDictionaries" />
					</editorArea:section>
					<editorArea:section name="sec.configuration.snindexconfiguration.general.session">
						<editorArea:attribute qualifier="user" />
						<editorArea:attribute qualifier="languages" />
						<editorArea:attribute qualifier="currencies" />
					</editorArea:section>
				</editorArea:tab>
				<editorArea:tab name="tab.configuration.snindexconfiguration.indextypes">
					<editorArea:section name="sec.configuration.snindexconfiguration.indextypes.indextypes">
						<editorArea:attribute qualifier="indexTypes" />
					</editorArea:section>
				</editorArea:tab>
			</editorArea:editorArea>
		</context>

		<context merge-by="type" parent="GenericItem" component="editorareaactions">
			<y:actions xmlns:y="http://www.hybris.com/cockpit/config/hybris">
				<y:group qualifier="common">
					<y:action action-id="com.hybris.cockpitng.action.delete" property="currentObject" />
					<y:action action-id="de.hybris.platform.searchbackoffice.actions.exportconfiguration"
						property="currentObject" />
				</y:group>
			</y:actions>
		</context>

		<context component="create-wizard">
			<wz:flow id="snindexconfiguration-create-wizard" title="create.title(ctx.TYPE_CODE)">
				<wz:prepare id="prepare">
					<wz:initialize property="newItem" type="ctx.TYPE_CODE" />
				</wz:prepare>
				<wz:step id="step1" label="create.snindexconfiguration.essential.label"
					sublabel="create.snindexconfiguration.essential.sublabel">
					<wz:content id="step1.content">
						<wz:property-list root="newItem">
							<wz:property qualifier="id" />
							<wz:property qualifier="name" />
							<wz:property qualifier="searchProviderConfiguration">
								<wz:editor-parameter>
									<wz:name>isNestedObjectCreationDisabled</wz:name>
									<wz:value>false</wz:value>
								</wz:editor-parameter>
							</wz:property>
						</wz:property-list>
					</wz:content>
					<wz:navigation id="step1.navigation">
						<wz:cancel />
						<wz:next visible="!#empty(newItem.id)" />
						<wz:done visible="!#empty(newItem.id)">
							<wz:save property="newItem" />
						</wz:done>
					</wz:navigation>
				</wz:step>
				<wz:step id="step2" label="create.snindexconfiguration.indextypes.label"
					sublabel="create.snindexconfiguration.indextypes.sublabel">
					<wz:content id="step2.content">
						<wz:property-list root="newItem">
							<wz:property qualifier="indexTypes" />
						</wz:property-list>
					</wz:content>
					<wz:navigation id="step2.navigation">
						<wz:cancel />
						<wz:back />
						<wz:next />
						<wz:done visible="!#empty(newItem.id)">
							<wz:save property="newItem" />
						</wz:done>
					</wz:navigation>
				</wz:step>
				<wz:step id="step3" label="create.snindexconfiguration.session.label"
					sublabel="create.snindexconfiguration.session.sublabel">
					<wz:content id="step3.content">
						<wz:property-list root="newItem">
							<wz:property qualifier="user" />
							<wz:property qualifier="languages" />
							<wz:property qualifier="currencies" />
						</wz:property-list>
					</wz:content>
					<wz:navigation id="step2.navigation">
						<wz:cancel />
						<wz:back />
						<wz:done visible="!#empty(newItem.id)">
							<wz:save property="newItem" />
						</wz:done>
					</wz:navigation>
				</wz:step>
			</wz:flow>
		</context>

						<editorArea:attribute qualifier="id" />
						<editorArea:attribute qualifier="name" />
						<editorArea:attribute qualifier="itemComposedType" />
						<editorArea:attribute qualifier="indexConfiguration" />
					</editorArea:essentialSection>
				</editorArea:essentials>
				<editorArea:tab name="tab.configuration.snindextype.general">
					<editorArea:section name="sec.configuration.snindextype.general.providers">
						<editorArea:attribute qualifier="identityProvider"
							editor="de.hybris.platform.searchbackoffice.editors.configurabledropdown">
							<editorArea:editor-parameter>
								<editorArea:name>dataProvider</editorArea:name>
								<editorArea:value>beansSnDataProvider</editorArea:value>
							</editorArea:editor-parameter>
							<editorArea:editor-parameter>
								<editorArea:name>dataProviderParameter_types</editorArea:name>
								<editorArea:value>de.hybris.platform.searchservices.core.service.SnIdentityProvider</editorArea:value>
							</editorArea:editor-parameter>
						</editorArea:attribute>
						<editorArea:attribute qualifier="identityProviderParameters" />
						<editorArea:attribute qualifier="defaultValueProvider"
							editor="de.hybris.platform.searchbackoffice.editors.configurabledropdown">
							<editorArea:editor-parameter>
								<editorArea:name>dataProvider</editorArea:name>
								<editorArea:value>beansSnDataProvider</editorArea:value>
							</editorArea:editor-parameter>
							<editorArea:editor-parameter>
								<editorArea:name>dataProviderParameter_types</editorArea:name>
								<editorArea:value>de.hybris.platform.searchservices.indexer.service.SnIndexerValueProvider
								</editorArea:value>
							</editorArea:editor-parameter>
						</editorArea:attribute>
						<editorArea:attribute qualifier="defaultValueProviderParameters" />
					</editorArea:section>
					<editorArea:section name="sec.configuration.snindextype.general.session">
						<editorArea:attribute qualifier="catalogs" />
						<editorArea:attribute qualifier="catalogVersions" />
					</editorArea:section>
				</editorArea:tab>
				<editorArea:tab name="tab.configuration.snindextype.fields">
					<editorArea:section name="sec.configuration.snindextype.fields.fields">
						<editorArea:panel name="FieldsPanel" colspan="4">
							<editorArea:attribute qualifier="fields"
								editor="com.hybris.cockpitng.editor.extendedmultireferenceeditor">
								<editorArea:editor-parameter>
									<editorArea:name>listConfigContext</editorArea:name>
									<editorArea:value>inLineEditorConfig</editorArea:value>
								</editorArea:editor-parameter>
								<editorArea:editor-parameter>
									<editorArea:name>inlineEditing</editorArea:name>
									<editorArea:value>true</editorArea:value>
								</editorArea:editor-parameter>
							</editorArea:attribute>
						</editorArea:panel>
					</editorArea:section>
				</editorArea:tab>
				<editorArea:tab name="tab.configuration.snindextype.cronjobs">
					<editorArea:section name="sec.configuration.snindextype.cronjobs">
						<editorArea:panel name="cronJobPanel" colspan="4">
							<editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea"
								editor="com.hybris.cockpitng.editor.extendedmultireferenceeditor" qualifier="indexerCronJobs">
								<editorArea:editor-parameter>
									<editorArea:name>createOnly</editorArea:name>
									<editorArea:value>true</editorArea:value>
								</editorArea:editor-parameter>
								<editorArea:editor-parameter>
									<editorArea:name>listConfigContext</editorArea:name>
									<editorArea:value>referenceListView</editorArea:value>
								</editorArea:editor-parameter>
								<editorArea:editor-parameter>
									<editorArea:name>selectedItemsMaxSize</editorArea:name>
									<editorArea:value>7</editorArea:value>
								</editorArea:editor-parameter>
							</editorArea:attribute>
						</editorArea:panel>
					</editorArea:section>
				</editorArea:tab>
			</editorArea:editorArea>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

    <bean class="de.hybris.platform.savedorderformsocc.dto.OrderFormWsDTO">
        <property name="code" type="String" />
        <property name="description" type="String" />
        <property name="currency" type="de.hybris.platform.commercewebservicescommons.dto.storesession.CurrencyWsDTO" />
        <property name="user" type="de.hybris.platform.commercewebservicescommons.dto.user.PrincipalWsDTO"/>
        <property name="entries" type="java.util.List&lt;de.hybris.platform.savedorderformsocc.dto.OrderFormEntryWsDTO>" />
    </bean>

    <bean class="de.hybris.platform.savedorderformsocc.dto.OrderFormEntryWsDTO">
        <property name="sku" type="String" />
        <property name="quantity" type="java.lang.Integer" />
    </bean>

    <bean class="de.hybris.platform.savedorderformsocc.dto.OrderFormListWsDTO">
        <property name="orderForms"
                  type="java.util.List&lt;de.hybris.platform.savedorderformsocc.dto.OrderFormWsDTO>"/>
    </bean>

</beans>

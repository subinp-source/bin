			<list-view:column qualifier="active"/>
			<list-view:column label="field.destinationtarget.id.description" qualifier="destinationTarget==null?'' : destinationTarget.id"/>
			<list-view:column label="field.endpoint.id.description" qualifier="endpoint==null?'' : endpoint.id" />
			<list-view:column label="field.abstractcredential.id.description" qualifier="credential==null?'' : credential" />
		</list-view:list-view>
	</context>

